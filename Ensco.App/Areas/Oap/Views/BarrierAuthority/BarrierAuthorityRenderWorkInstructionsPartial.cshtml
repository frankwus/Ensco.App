@model IEnumerable<Ensco.Irma.Oap.WebClient.Rig.RigOapChecklistWorkInstruction>

@{
    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();

    Html.DevExpress().GridView(settings =>
    {
        var oapController = (dynamic) ViewContext.Controller;
         
        oapController.ConfigureWorkInstructions(settings, Html, ViewContext);

    }).Bind(Model).GetHtml();
}




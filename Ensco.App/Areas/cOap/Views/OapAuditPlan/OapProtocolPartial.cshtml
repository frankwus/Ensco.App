@model IEnumerable<Ensco.App.Areas.cOap.Models.AuditPlanProtocolModel> 

@{
    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();

    var oapAuditPlanSummaryController = ViewContext.Controller as Ensco.App.Areas.cOap.Controllers.OapAuditPlanController;

    var grid = Html.DevExpress().GridView(settings =>
    {
        oapAuditPlanSummaryController.ConfigureProtocols(settings, Html, ViewContext);
    }).Bind(Model); 

    grid.GetHtml();
}



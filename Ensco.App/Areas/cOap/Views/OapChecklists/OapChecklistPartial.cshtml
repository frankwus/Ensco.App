@model IEnumerable<Ensco.Irma.Oap.WebClient.Corp.OapChecklist>

@{

    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();

    Html.DevExpress().GridView(settings =>
    {
        var oapGroupGroupController = ViewContext.Controller as Ensco.App.Areas.cOap.Controllers.OapChecklistsController;
        oapGroupGroupController.Configure(settings, Html, ViewContext);



    }).Bind(Model).GetHtml();
}



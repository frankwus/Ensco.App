@model IEnumerable<Ensco.Irma.Oap.WebClient.Corp.OapChecklistQuestionTag>
@{
    ViewBag.Title = "Oap Question Tags";
}


@section scripts
{
    <script type="text/javascript">

        function onToolbarItemClick(s, e) {
            switch (e.item.name) {
                case "AddNewQuestionTag":
                    s.AddNewRow();
                    break;
            }
        }
    </script>
}


@*<h1 class="area-headers">@ViewBag.Title</h1>*@
<div class="oap-master-grid">
    <form>
        @Html.Partial("OapQuestionTagPartial", Model)
    </form>      
</div>


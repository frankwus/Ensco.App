@model IEnumerable<Ensco.Irma.Oap.WebClient.Corp.OapFrequencyType>
@{
    ViewBag.Title = "Oap Freqeuncy Types";
}


@section scripts
{
    <script type="text/javascript">

        function onToolbarItemClick(s, e) {
            switch (e.item.name) {
                case "AddNewFrequencyType":
                    s.AddNewRow();
                    break;
            }
        }
    </script>
}


@*<h1 class="area-headers">@ViewBag.Title</h1>*@
<div class="oap-master-grid">
    <form>
        @Html.Partial("OapFrequencyTypePartial", Model)
    </form>   
</div>

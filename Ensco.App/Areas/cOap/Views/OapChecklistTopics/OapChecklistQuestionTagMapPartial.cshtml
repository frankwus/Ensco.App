@model IEnumerable<Ensco.Irma.Oap.WebClient.Corp.OapChecklistQuestionTagMap>

@{
    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();

    Html.DevExpress().GridView(settings =>
   {
       var oapController = ViewContext.Controller as Ensco.App.Areas.cOap.Controllers.OapChecklistTopicsController;
       oapController.ConfigureTagSubChild(settings, Html, ViewContext);
   }).Bind(Model).GetHtml();
}